# Replicate API Configuration
# Get your API token from: https://replicate.com/account/api-tokens
REPLICATE_API_TOKEN=your_replicate_api_token_here

# Next.js Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000

# AI API Keys (Add these when you get them)
# Claude API - https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Google AI API - https://makersuite.google.com/app/apikey
GOOGLE_AI_API_KEY=your_google_ai_api_key_here

# Google Cloud API - https://console.cloud.google.com/
GOOGLE_CLOUD_PROJECT_ID=your_project_id_here
GOOGLE_CLOUD_CREDENTIALS_PATH=path/to/service-account-key.json

# OpenAI API (Alternative to Claude)
OPENAI_API_KEY=your_openai_api_key_here

# Spotify Web API (For music analysis)
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# Audio Analysis Configuration
ENABLE_REAL_AUDIO_ANALYSIS=false
DEFAULT_ANALYSIS_PROVIDER=mock
